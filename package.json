    "scripts": {
        "dev": "next",
        "predev": "yarn codegen",
        "start": "next start",
        "build": "next build && next export",
        "prebuild": "yarn codegen",
        "type-check": "tsc",
        "codegen": "DOTENV_CONFIG_PATH=./.env graphql-codegen -r dotenv/config --config codegen.yml",
        "codegen:watch": "yarn codegen -- --watch",
        "lint:fix": "eslint --fix --cache --ext .ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less"
    },
    "dependencies": {
        "@apollo/client": "^3.4.16",
        "autoprefixer": "^10.3.7",
        "dayjs": "^1.10.7",
        "graphql": "^15.6.1",
        "graphql-tag": "^2.12.5",
        "lodash": "^4.17.21",
        "next": "11.1.2",
        "next-ga": "^2.3.4",
        "next-i18next": "^8.9.0",
        "next-with-apollo": "^5.2.1",
        "nprogress": "^0.2.0",
        "postcss": "^8.3.11",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.12.0",
        "tailwindcss": "^2.2.17"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.2.1",
        "@graphql-codegen/import-types-preset": "^2.1.6",
        "@graphql-codegen/introspection": "^2.1.0",
        "@graphql-codegen/plugin-helpers": "^2.2.0",
        "@graphql-codegen/typescript": "^2.2.4",
        "@graphql-codegen/typescript-operations": "^2.1.8",
        "@graphql-codegen/typescript-react-apollo": "^3.1.6",
        "@next/eslint-plugin-next": "^11.1.2",
        "@types/lodash": "^4.14.175",
        "@types/node": "^16.11.1",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^17.0.30",
        "@types/react-dom": "^17.0.10",
        "@umijs/fabric": "^2.8.1",
        "babel-plugin-import": "^1.13.3",
        "dotenv": "^10.0.0",
        "i18next": "^21.3.3",
        "i18next-browser-languagedetector": "^6.1.2",
        "i18next-xhr-backend": "^3.2.2",
        "typescript": "4.4.4"
    }
